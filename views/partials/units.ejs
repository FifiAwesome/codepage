<% units.forEach(function(unit){ %>
  <div class="four wide column">
      <div class="unit-icon">
      <a href="/units/<%=unit._id%>">
      <img src="<%= unit.url %>">
      <h4><%=unit.name%></h4></a>
      <form class="formdel" method="POST" action="/units/<%=unit._id%>?_method=DELETE">
      <button class="ui icon button medium"><i class="trash icon"></i></button>
      </form>
    </div>
  </div>

<% }); %>
<script>

$("a").on("click", function(ev){
  ev.preventDefault();
  var attr = $(this).attr("href")
  $("#link-list").load(attr);
})
</script>
